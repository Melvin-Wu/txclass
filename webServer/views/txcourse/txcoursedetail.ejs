	
<div class="row">
	<div class="col-xs-12">
		<div class="table-responsive">
			<table id="link_officer" class="table table-striped table-bordered table-hover data_table2">
				<thead>
					<tr>
						<th>课程id</th>
						<th>年级</th>
						<th>课程名</th>
						<th>课程计划</th>
						<th>最大价格(元)</th>
						<th>最小价格(元)</th>
						<th>授课老师</th>
						<th>辅导老师</th>
					</tr>
				</thead>
				<tbody>
					<% for(var i = 0; i < data_list.length; ++i){%>
						<tr>
							<td><%= data_list[i].cid %></td>
							<td>
								<% if(data_list[i].grade == 8001){%>
										幼儿园小班
									<%}else if(data_list[i].grade == 8002){%>
										幼儿园中班
									<%}else if(data_list[i].grade == 8003){%>
										幼儿园大班
									<%}else if(data_list[i].grade == 7001){%>
										小学一年级
									<%}else if(data_list[i].grade == 7002){%>
										小学二年级
									<%}else if(data_list[i].grade == 7003){%>
										小学三年级
									<%}else if(data_list[i].grade == 7004){%>
										小学四年级
									<%}else if(data_list[i].grade == 7005){%>
										小学五年级
									<%}else if(data_list[i].grade == 7006){%>
										小学六年级
									<%}else if(data_list[i].grade == 6001){%>
										初一
									<%}else if(data_list[i].grade == 6002){%>
										初二
									<%}else if(data_list[i].grade == 6003){%>
										初三
									<%}else if(data_list[i].grade == 5001){%>
										高一
									<%}else if(data_list[i].grade == 5002){%>
										高二
									<%}else if(data_list[i].grade == 5003){%>
										高三
									<%}else {%>
										其它
								<%}%>
							</td>
							<td><%= data_list[i].name %></td>
							<td><%= data_list[i].time_plan %></td>
							<td><%= data_list[i].course_min_price/100 %></td>
							<td><%= data_list[i].course_max_price/100 %></td>
							<td><%= data_list[i].te_name %></td>
							<td><%= data_list[i].tu_name %></td>
						</tr>
					<%}%>
				</tbody>
			</table>
		</div>
	</div>
</div>	
